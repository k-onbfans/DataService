<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.accenture.data.entity.FlightInfoEntity" table="flight_info" schema="integrated_system">
        <id name="id" column="id"/>
        <property name="flightNumber" column="flight_number"/>
        <property name="originPort" column="origin_port"/>
        <property name="destinationPort" column="destination_port"/>
        <property name="aircraft" column="aircraft"/>
    </class>
</hibernate-mapping>